    "# Example 4: Resetting the Dimensions of Data Using NumPy\n",
    "\n",
    "# importing library\n",
    "import numpy as np\n",
    "\n",
    "# initializing list\n",
    "lst = [1, 7, 0, 5, 6, 2, 5, 6]\n",
    "\n",
    "# converting list to array\n",
    "arr = np.array(lst)\n",
    "\n",
    "# reshape array to 2 rows by four columns\n",
    "arr = np.reshape(arr,(2,4))\n",
    "\n",
    "print(arr)"
   ]
  }
